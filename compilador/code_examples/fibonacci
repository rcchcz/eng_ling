# Função para retornar o x-ésimo elemento da sequência de Fibonacci
def int fibonacci(int x) {
    int antepNum;
    int penNum = 0;
    int numAtual = 1;

  if (x == 0 || x == 1) {
       return x;
  }

  for (int i : [2, x + 1]) {
    antepNum = penNum;
    penNum = numAtual;
    numAtual = antepNum + penNum;
  }

  return numAtual;
}